{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "src/**/*",
          "tsconfig.json"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "functions": {
    "src/main.ts": {
      "runtime": "@vercel/node"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}